{
  "name": "@slushy/example",
  "version": "0.10.0",
  "main": "dist/index.js",
  "repository": "https://github.com/aimed/slushy",
  "author": "Maximilian Taeschner",
  "license": "MIT",
  "private": true,
  "prettier": "../.prettierrc.json",
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@slushy/codegen": "^0.10.0",
    "@types/jest": "^24.0.15",
    "@types/supertest": "2.0.7",
    "jest": "24.8.0",
    "lint-staged": "8.1.6",
    "prettier": "1.18.2",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "typescript": "3.4.5"
  },
  "dependencies": {
    "@slushy/server": "^0.10.0"
  },
  "scripts": {
    "test": "yarn generate && yarn jest",
    "generate": "node node_modules/@slushy/codegen/dist/index.js gen ./pet.yaml ./src/generated",
    "build": "yarn generate && tsc --noEmit",
    "start": "ts-node src/index.ts"
  }
}
